<script language="JavaScript">
    /** User our own callback function to update the profile link when the user has been retrieved from the server **/
    function updateProfileLink(user){
        /** Make sure user has been retrieved before updating the link **/  
        if (!user.isEmpty()){
            $('#anonymous').hide();
            $('#loggedin').show();
            $('a.welcome').text('Hello,  ' + user.getFirstname());
            $('#logout').text('Logout');
        } else {
            $('#loggedin').hide();          
            $('#anonymous').show();
        }
        
    }

	
    //login-dropdown
    var lDropDown = {
        ddIsOpen: false,
        initalize: function() {
            $('.login-dropdown').click(function (e) {
                e.preventDefault();
                if (lDropDown.ddIsOpen) {
                    lDropDown.removeDD($(this));
                } else {
                    lDropDown.displayDD($(this));
                }           
            });
        },
        displayDD: function(mObj) {
            $(mObj).addClass('dropdown-li');
            $(mObj).addClass('dropdown-a');
            
            floatDiv = "<div id='login-dropdown'>" + $('#dropdown-content').html() + "</div>";
            
            cW = $('.login-dropdown').width() + 14;
            cW = (cW < 55) ? 55 : cW;
            cT = $('.login-dropdown').offset().top + $('.login-dropdown').height() + 2;
            cL = $('.login-dropdown').position().left + $('#nav-utilitytop').position().left + 1;       
            
            $('#header').append(floatDiv);
            $('#login-dropdown').css({'top' : '7px', 'left' : cL + 'px', 'width' : cW + 'px', 'min-width' : '95px'});
            $('a.welcome').css({'font-weight' : 'bold'});
            
            lDropDown.ddIsOpen = true;
        },
        removeDD: function(mObj) {
            $('#login-dropdown').remove();
            $(mObj).removeClass('dropdown-li');
            $(mObj).removeClass('dropdown-a');
            lDropDown.ddIsOpen = false;
        }
    }
    
    
    $(window).load(function () {
        lDropDown.initalize();
        $('.closeMe').click(function (e) {
	       	 $('#login-dropdown').remove();
	       });
		/** Get some Javascript objects that represent the user that has logged in **/
	    var userFactory = new WCMS.IDS.Users.UserFactory();
	    userFactory.getUser(updateProfileLink);
    });
	
	$(window).click(function(evt) {
        var target = evt.target;
        var inside = $("#loggedin a");
        if (target.className != "login-dropdown welcome dropdown-li dropdown-a") {
           if(lDropDown && lDropDown.ddIsOpen)
           {
               lDropDown.removeDD(inside);
           }
    }
   });
        
</script>


    
